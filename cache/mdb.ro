 . f  s i=$o(subscripts(i)) q:i=""  d
 . . s ref=ref_comma_""""_subscripts(i)_""""
 . . s comma=","
 . s ref=ref_")"
 s ref=ref_"="""_value_""""
 x ref
 i $g(%KEY("OutputFormat"))="JSON" s response(1)="{""ok"":true}"
