 . f  s i=$o(subscripts(i)) q:i=""  d
 . . s ref=ref_comma_""""_subscripts(i)_""""
 . . s comma=","
 . s ref=ref_comma
 s refx=ref
 s ref=ref_""""_seedValue_""")"
 s ref="s next=$o("_ref_")"
 x ref
 s next(1)=next
 f i=2:1 s next=$o(^(next)) q:next=""  s next(i)=next
 ;
 i '$d(next(2)) d
 . s json="[]"
 e  d
 . s json="[",comma=""
