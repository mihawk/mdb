 s ref="^"_globalName_"("
 i $d(subscripts) d
 . s comma=""
 . s i=""
 . f  s i=$o(subscripts(i)) q:i=""  d
 . . s ref=ref_comma_""""_subscripts(i)_""""
 . . s comma=","
 . s ref=ref_comma
 s refx=ref
